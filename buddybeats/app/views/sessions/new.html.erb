<h1>Log-in</h1>

<%= form_tag sessions_path do %>



<%= label_tag :user_email %>
<%= text_field_tag :user_email %>
<%= label_tag :password %>
<%= password_field_tag :password %>
<%= submit_tag "Log-in" %>
<% end %>

<p><hr>
  <h3>Ignore below</h3>
if @user.errors.any?
@user.errors.full_messages.each do |message|
message
@customError = "Sorry, wrong username or password."
end
end
